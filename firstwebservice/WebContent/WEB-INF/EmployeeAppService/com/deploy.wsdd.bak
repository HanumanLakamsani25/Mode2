<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EmployeeAppService WSDL service -->

  <service name="EmployeeApp" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com"/>
      <parameter name="wsdlServiceElement" value="EmployeeAppService"/>
      <parameter name="schemaQualified" value="http://model.com,http://com"/>
      <parameter name="wsdlServicePort" value="EmployeeApp"/>
      <parameter name="className" value="com.EmployeeAppSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EmployeeApp"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getEmployeeById" qname="operNS:getEmployeeById" xmlns:operNS="http://com" returnQName="retNS:getEmployeeByIdReturn" xmlns:retNS="http://com" returnType="rtns:Employee" xmlns:rtns="http://model.com" soapAction="" >
        <parameter qname="pns:empId" xmlns:pns="http://com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getEmployeeById"/>

      <typeMapping
        xmlns:ns="http://model.com"
        qname="ns:Employee"
        type="java:com.model.Employee"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
